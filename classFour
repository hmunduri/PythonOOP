#Data Structures:

#List

#List Declaration:

#name_list = []
name_list = ['krishna', 4, 'GUNTUKA', '5.7'] 

name_list.append("krishna")
name_list.append("GUNTUKA")

name_list[1] = 25

print(len(name_list))
print(name_list[0])

#list index starts from 0

print(name_list[1])
print(name_list)
print(name_list[2])

 
for item in name_list:
    print(item)
